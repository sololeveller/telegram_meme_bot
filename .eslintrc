{
  "extends": "airbnb-base",
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jasmine": true,
    "mocha": true,
    "mongo": true
  },
  "rules": {
    "new-cap": 0,
    "indent": 0,
    "prefer-const": 0,
    "no-undef": 0,
    "no-underscore-dangle": 0,
    "no-param-reassign": 0,
    "no-unused-vars": 0,
    "import/prefer-default-export": 0,
    "import/no-extraneous-dependencies": 0,
    "import/newline-after-import": 0,
    "import/no-unresolved": 0,
    "import/extensions": 0,
    "import/imports-first": 0,
    "no-useless-escape": 0,
    "consistent-return": 0,
    "max-len": ["error", 200, 2, {
      "ignoreUrls": true,
      "ignoreComments": false,
      "ignoreStrings": true,
      "ignoreTemplateLiterals": true
    }],
    "no-plusplus": 0,
    "no-use-before-define": 0,
    "quote-props": 0,
    "arrow-body-style": 0
  }
}
